<templateSet group="logger">
  <template name="loggerd" value="timber.log.Timber.d(&quot;$METHOD_NAME$: $content$&quot;);" description="" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggere" value="timber.log.Timber.e(e, &quot;$METHOD_NAME$: $content$&quot;);" description="" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggeri" value="timber.log.Timber.i(&quot;$METHOD_NAME$: $content$&quot;);" description="Log.i(TAG, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggerr" value="timber.log.Timber.d(TAG, &quot;$METHOD_NAME$() returned: &quot; +  $result$);" description="Log result of this method" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="result" expression="variableOfType(methodReturnType())" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggervst" value="timber.log.Timber.v(new Throwable(), &quot;$METHOD_NAME$: $content$&quot;);" description="log v with stacktrace" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="loggerw" value="timber.log.Timber.w(&quot;$METHOD_NAME$: $content$&quot;);" description="Log.w(TAG, String, Exception)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggerwtf" value="timber.log.Timber.wtf($exception$, &quot;$METHOD_NAME$: $content$&quot;);" description="Log.wtf(TAG, String, Exception)" toReformat="true" toShortenFQNames="true">
    <variable name="exception" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggerv" value="timber.log.Timber.v(&quot;$METHOD_NAME$: $content$&quot;);" description="" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggerdd" value="timber.log.Timber.d($content$);" description="log with parameters" toReformat="true" toShortenFQNames="true">
    <variable name="content" expression="groovyScript(&quot;E:\\\\thupten\\\\development\\\\transitbus\\\\logger_live_template.groovy&quot;, methodName(), methodParameters())" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggervv" value="timber.log.Timber.v($content$);" description="log with parameters" toReformat="true" toShortenFQNames="true">
    <variable name="content" expression="groovyScript(&quot;E:\\\\thupten\\\\development\\\\transitbus\\\\logger_live_template.groovy&quot;, methodName(), methodParameters())" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="loggerii" value="timber.log.Timber.i($content$);" description="log with parameters" toReformat="true" toShortenFQNames="true">
    <variable name="content" expression="groovyScript(&quot;E:\\\\thupten\\\\development\\\\transitbus\\\\logger_live_template.groovy&quot;, methodName(), methodParameters())" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>